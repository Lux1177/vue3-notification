{
  "name": "@iwannabecool/vue-notification",
  "description": "Vue.js Notification Library",
  "version": "2.0.3-0",
  "author": "iwannabecool",
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build:base": "webpack --config webpack.config.js --progress",
    "build:ssr": "webpack --config webpack.config.ssr.js --progress",
    "build": "npm run build:base && npm run build:ssr",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/setup/karma.conf.js --single-run",
    "unit:watch": "cross-env BABEL_ENV=test karma start test/unit/setup/karma.conf.js --watch",
    "test": "npm run unit",
    "lint": "eslint ./src",
    "version": "npm run build && git add -A dist"
  },
  "types": "index.d.ts",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/iwannabecool/vue-notification.git"
  },
  "bugs": {
    "url": "https://github.com/iwannabecool/vue-notification/issues"
  },
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.11"
  },
  "keywords": [
    "web",
    "front-end",
    "vue",
    "vuejs",
    "notification",
    "vue-notification",
    "vue-notifications"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vue/compiler-sfc": "^3.0.11",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.1",
    "eslint": "^7.24.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.9.0",
    "file-loader": "^6.2.0",
    "karma": "^6.3.2",
    "karma-coverage": "^2.0.3",
    "karma-mocha": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^5.0.0",
    "mocha": "^8.3.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "typescript": "^4.2.4",
    "vue": "^3.0.11",
    "vue-loader": "^16.2.0",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "4.0.0-beta.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "mitt": "^2.1.0"
  }
}
